{
  "name": "react-modern-transform",
  "type": "commonjs",
  "version": "0.0.0",
  "description": "",
  "exports": {
    "./tsx": {
      "import": {
        "types": "./__compiled__/esm/tsx.d.mts",
        "default": "./__compiled__/esm/tsx.mjs"
      },
      "require": {
        "types": "./__compiled__/cjs/tsx.d.ts",
        "default": "./__compiled__/cjs/tsx.js"
      },
      "types": "./__compiled__/cjs/tsx.d.ts",
      "default": "./__compiled__/cjs/tsx.js"
    },
    "./js": {
      "import": {
        "default": "./__compiled__/esm/js.mjs"
      },
      "require": {
        "default": "./__compiled__/cjs/js.js"
      },
      "default": "./__compiled__/cjs/js.js"
    },
    "./jsx": {
      "import": {
        "default": "./__compiled__/esm/jsx.mjs"
      },
      "require": {
        "default": "./__compiled__/cjs/jsx.js"
      },
      "default": "./__compiled__/cjs/jsx.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "react": "^17.0.0",
    "@types/react": "^17.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}